{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Debug",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/src/AnvilPacker/bin/Debug/AnvilPacker.dll",
            "args": [],
            "cwd": "${workspaceFolder}/src/AnvilPacker/bin/Debug/",
            "console": "internalConsole",
            "stopAtEntry": false,
            "suppressJITOptimizations": true
        },
        {
            "name": "Launch Release",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "buildRelease",
            "program": "${workspaceFolder}/src/AnvilPacker/bin/Release/AnvilPacker.dll",
            "args": [],
            "cwd": "${workspaceFolder}/src/AnvilPacker/bin/Release/",
            "console": "internalConsole",
            "stopAtEntry": false,
            "suppressJITOptimizations": false
        },
        {
            "name": ".NET Core Attach",
            "type": "coreclr",
            "request": "attach",
            "processId": "${command:pickProcess}"
        }
    ]
}